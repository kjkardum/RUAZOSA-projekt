<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homepageFragment3">
    <fragment
        android:id="@+id/homepageFragment3"
        android:name="hr.fer.ruazosa.kviz2022.homepage.HomepageFragment"
        android:label="HomepageFragment" >
        <action
            android:id="@+id/action_homepageFragment3_to_leaderboardFragment3"
            app:destination="@id/leaderboardFragment3"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/slide_out"
            app:popUpTo="@id/homepageFragment3" />
    </fragment>
    <fragment
        android:id="@+id/leaderboardFragment3"
        android:name="hr.fer.ruazosa.kviz2022.leaderboard.LeaderboardFragment"
        android:label="LeaderboardFragment" >
        <action
            android:id="@+id/action_leaderboardFragment3_to_homepageFragment3"
            app:destination="@id/homepageFragment3"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
</navigation>